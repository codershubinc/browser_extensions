{
    "manifest_version": 3,
    "name": "Year Countdown",
    "description": "Shows time left until the end of the year.",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "./newtab/newtab.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "ico.png",
        "48": "ico.png",
        "128": "ico.png"
    }
}